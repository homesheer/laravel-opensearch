{
    "name": "homesheer/laravel-opensearch",
    "description": "阿里云开放搜索适配Laravel5",
    "type": "library",
    "keywords": [
        "laravel",
        "opensearch"
    ],
    "homepage": "http://homesheer.com/laravel-opensearch",
    "license": "MIT",
    "authors": [
        {
            "name": "homesheer",
            "email": "homesheer@qq.com",
            "homepage": "http://homesheer.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.8",
        "laravel/scout": "~7.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.6"
    },
    "autoload": {
        "psr-4": {
            "HomeSheer\\OpenSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HomeSheer\\OpenSearch\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "HomeSheer\\OpenSearch\\OpenSearchServiceProvider"
            ]
        }
    }
}
